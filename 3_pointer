// 3 pointer using DUTCH NATIONAL FLAG SEG
#include <bits/stdc++.h>
void sortArray(vector<int>& arr, int n){
int low = 0, mid = 0, high = n-1;
while(mid<=high){
if(arr[nid] == 0){
swap(arr[mid] . arr[low]);
low++;
mid++;
}
else if(arr[mid] == 1){
mid++;
}
else{
swap(arr[high] , arr[mid]);
high--;
}
}

}
